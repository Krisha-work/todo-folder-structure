    "/api/user/register": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "example": "testuser"
                },
                "email": {
                  "example": "test@gmail.com"
                },
                "password": {
                  "example": "test@12345"
                },
                "contact": {
                  "example": "9876543210"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "examples": {
              "application/json": {
                "message": "Signup data inserted successfully",
                "username": "testuser",
                "email": "test@gmail.com",
                "password": "$2a$08$ARPbubLyQH6XfMq.cfuEBeXdjawfNKhmdp6VlwkVgJMqgXnZzp1Zm",
                "contact": "9876543210"
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "examples": {
              "application/json": {
                "message": "Username is requrie"
              }
            }
          },
          "409": {
            "description": "Conflict",
            "examples": {
              "application/json": {
                "message": "User already exists",
                "existingUser": [
                  {
                    "id": "<UserId_from_database>",
                    "username": "testuser",
                    "email": "test@gmail.com",
                    "password": "$2a$08$ARPbubLyQH6XfMq.cfuEBeXdjawfNKhmdp6VlwkVgJMqgXnZzp1Zm",
                    "contact": "9876543210"
                  }
                ]
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "examples": {
              "application/json": {
                "message": "Somthing went wrong in SignUp."
              }
            }
          }
        }
      }
    },
